{
    "name": "nettixcode/framework",
    "description": "The skeleton application for the NettixCode framework",
    "type": "library",
    "version": "1.2.4",
    "license": "MIT",
    "authors": [
        {
            "name": "NettixCode",
            "email": "NettixCode@gmail.com",
            "homepage": "https://github.com/NettixCode"
        }
    ],
    "autoload": {
        "files": [
            "Src/Foundation/helpers.php",
            "Libraries/Illuminate/Collections/helpers.php",
            "Libraries/Illuminate/Events/functions.php",
            "Libraries/Illuminate/Filesystem/functions.php",
            "Libraries/Illuminate/Support/helpers.php"
        ],
        "psr-4": {
            "Nettixcode\\Framework\\": "Src",
            "Laravel\\SerializableClosure\\": "Libraries/serializable-closure/src",
            "Illuminate\\": "Libraries/Illuminate",
            "Illuminate\\Support\\": [
                "Libraries/Illuminate/Macroable",
                "Libraries/Illuminate/Collections",
                "Libraries/Illuminate/Conditionable"
            ]
        }
    },
    "require": {
        "php": "^8.2",
        "phpoffice/phpspreadsheet": "^2.1",
        "vlucas/phpdotenv": "^5.6",
        "firebase/php-jwt": "^6.10",
        "maximebf/debugbar": "^1.22",
        "brick/math": "^0.9.3|^0.10.2|^0.11|^0.12",
        "egulias/email-validator": "^3.2.5|^4.0",
        "fruitcake/php-cors": "^1.3",
        "guzzlehttp/guzzle": "^7.8",
        "guzzlehttp/uri-template": "^1.0",
        "monolog/monolog": "^3.0",
        "psr/log": "^1.0|^2.0|^3.0",
        "psr/container": "^1.1.1|^2.0.1",
        "psr/simple-cache": "^1.0|^2.0|^3.0",
        "doctrine/inflector": "^2.0",
        "nesbot/carbon": "^2.72.2|^3.0",
        "voku/portable-ascii": "^2.0",
        "symfony/finder": "^7.0",
        "symfony/http-foundation": "^7.0",
        "symfony/http-kernel": "^7.0",
        "symfony/process": "^7.0",
        "symfony/routing": "^7.0",
        "symfony/polyfill-php83": "^1.28",
        "symfony/mime": "^7.0"
    }
}